= form_for @guest do |f|
  = f.error_messages
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :address
    %br
    = f.text_field :address
  %p
    = f.label :side
    %br
    = f.text_field :side
  %p
    = f.label :is_adult
    %br
    = f.check_box :is_adult
  %p
    = f.label :status
    %br
    = f.check_box :status
  %p
    = f.submit
